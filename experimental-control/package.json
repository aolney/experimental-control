{
  "name": "experimental-control",
  "version": "0.1.0",
  "description": "DataWhys JupyterLab experimental control extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/memphis-iis/experimental-control",
  "bugs": {
    "url": "https://github.com/memphis-iis/experimental-control/issues"
  },
  "license": "BSD-3-Clause",
  "author": "",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/memphis-iis/experimental-control.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "chokidar": "^3.4.3",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.3",
    "rimraf": "^2.6.1",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
